{
  "name": "nextjs-cdk",
  "version": "1.0.0",
  "author": "Anders Quist <anders.quist@polestar.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json && yarn copy-sharp-modules && yarn rollup",
    "clean": "yarn rimraf dist",
    "copy-sharp-modules": "ts-node --project tsconfig.scripts.json scripts/copy-sharp-modules.ts",
    "format": "prettier --ignore-path .gitignore --write src tests",
    "rollup": "rollup --config",
    "pretest": "yarn build",
    "test": "yarn test:integration && yarn test:e2e",
    "test:unit": "jest --verbose ./tests/unit",
    "test:e2e": "ts-node --project tsconfig.scripts.json ./tests/e2e/run-tests.ts",
    "test:integration": "jest --verbose ./tests/integration"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/aws-lambda": "^8.10.85",
    "@types/cookie": "^0.4.1",
    "@types/fresh": "^0.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@types/normalize-path": "^3.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/send": "^0.17.1",
    "@types/sharp": "^0.29.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "<8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "zx": "^4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.39.0",
    "@aws-sdk/client-sqs": "^3.39.0",
    "cookie": "^0.4.1",
    "execa": "^5.1.1",
    "fresh": "^0.5.2",
    "fs-extra": "^10.0.0",
    "is-animated": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^12.0.2",
    "node-fetch": "^3.0.0",
    "normalize-path": "^3.0.0",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "send": "^0.17.1",
    "sharp": "^0.29.2"
  }
}
